# Discord Bot Configuration
# Copy this file to .env and fill in your values

# Required: Your Discord bot token
DISCORD_TOKEN=your_discord_bot_token_here

# Required: Your Discord server ID
DISCORD_SERVER_ID=your_server_id_here

# Optional: Discord channel/role IDs (uses defaults if not set)
PRIVATE_CHAT_CATEGORY_ID=
CAT_CAFE_CHANNEL_ID=
LOGS_CHANNEL_ID=
VERIFICATION_MESSAGE_ID=
VERIFICATION_CHANNEL_ID=

# Optional: URL shortener APIs for ad monetization
SHRINKME_API_KEY=your_shrinkme_api_key_here
SHRINKEARN_API_KEY=your_shrinkearn_api_key_here

# Optional: HTTP proxy for scraping (useful if VPS IP is blocked)
# Format: http://username:password@host:port
PROXY_URL=

# Optional: Rotating proxy for heavy-security sites (ranobes, novelbin, etc.)
# For Webshare: http://username-rotate:password@p.webshare.io:80
# This gets a new IP per request, helping avoid rate limits
ROTATING_PROXY_URL=
